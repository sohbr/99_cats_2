<h1>Existing User Log In</h1>

<% if flash[:errors] %>
  <ul>
    <% flash[:errors].each do |error| %>
      <li><%= error%></li>
    <% end %>
  </ul>
<% end %>


<form action=" <%= session_url %> " method="post">


  Username:
  <input type="text" name="user[username]">

  Password:
  <input type="password" name="user[password]">

  <input type="submit" value="Sign In">

</form>
